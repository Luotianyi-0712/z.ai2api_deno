# Default configuration for docker-compose quick start
# Copy or edit these values to match your environment.

# API Configuration
API_ENDPOINT=https://chat.z.ai/api/chat/completions
AUTH_TOKEN=sk-your-api-key
BACKUP_TOKEN=token-1,token-2

# Model Configuration
PRIMARY_MODEL=GLM-4.5
THINKING_MODEL=GLM-4.5-Thinking
SEARCH_MODEL=GLM-4.5-Search
AIR_MODEL=GLM-4.5-Air
PRIMARY_MODEL_NEW=GLM-4.6
THINKING_MODEL_NEW=GLM-4.6-Thinking
SEARCH_MODEL_NEW=GLM-4.6-Search

# Server Configuration
LISTEN_PORT=8080
DEBUG_LOGGING=true

# Feature Configuration
THINKING_PROCESSING=think
ANONYMOUS_MODE=true
TOOL_SUPPORT=true
SCAN_LIMIT=200000
SKIP_AUTH_TOKEN=false
